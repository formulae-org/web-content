pesetting.Paragraph"><expression tag="String.Text" Value="67 / 74 = 0.9(054) = 0.9054054..."/></expression><expression tag="String.Text" Value="14 / 27 = 0.(518) = 0.518518..."/></expression><expression tag="Typesetting.Paragraph"><expression tag="String.Text" Value="Finite decimals are of course no problem:"/></expression><expression tag="Typesetting.BulletedList"><expression tag="Typesetting.Paragraph"><expression tag="String.Text" Value="0.75 → 3 / 4"/></expression></expression><expression tag="Typesetting.Paragraph"><expression tag="Typesetting.BoldChunk"><expression tag="String.Text" Value="Solution"/></expression></expression><expression tag="Typesetting.Paragraph"><expression tag="Typesetting.BoldChunk"><expression tag="String.Text" Value="Case 1. Without repeating digits"/></expression></expression><expression tag="Typesetting.Paragraph"><expression tag="String.Text" Value="Fōrmulæ"/><expression tag="Internet.UniformResourceLocator" Value="/?reference=Math.Arithmetic.Rationalize" Description="Rationalize"/><expression tag="String.Text" Value="expression converts from decimal numberts to rational in lower terms."/></expression></expression></expression><expression tag="Formulae.Script.Row" type="0"><expression tag="List.List"><expression tag="List.List"><expression tag="Math.Arithmetic.Rationalize"><expression tag="Math.Number" Value="0.75"/></expression></expression><expression tag="List.List"><expression tag="Math.Arithmetic.Rationalize"><expression tag="Math.Number" Value="0.518518"/></expression></expression><expression tag="List.List"><expression tag="Math.Arithmetic.Rationalize"><expression tag="Math.Number" Value="0.9054054"/></expression></expression><expression tag="List.List"><expression tag="Math.Arithmetic.Rationalize"><expression tag="Math.Number" Value="0.142857143"/></expression></expression><expression tag="List.List"><expression tag="Math.Arithmetic.Rationalize"><expression tag="Math.Number" Value="3.141592654"/></expression></expression><exp