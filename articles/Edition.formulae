						<expression tag="String.Text" Value="If the selected expression is the null expression:"/>
					</expression>
					<expression tag="Typesetting.BulletedList">
						<expression tag="Typesetting.Paragraph">
							<expression tag="String.Text" Value="If the nature of its parent expression &quot;accepts&quot; having one less subexpression, the selected (null) expression is just deleted. For example the expression"/>
							<expression tag="Math.Arithmetic.Addition">
								<expression tag="Math.Number" Value="2"/>
								<expression tag="Visualization.Selected">
									<expression tag="Null"/>
								</expression>
								<expression tag="Math.Number" Value="4"/>
							</expression>
							<expression tag="String.Text" Value="becomes"/>
							<expression tag="Math.Arithmetic.Addition">
								<expression tag="Math.Number" Value="2"/>
								<expression tag="Math.Number" Value="4"/>
							</expression>
							<expression tag="String.Text" Value="after the deletion. However, in the following example:"/>
