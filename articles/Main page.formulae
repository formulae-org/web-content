<expression tag="Formulae.Script" Modules="org.formulae.typesetting,org.formulae.graphics.raster,org.formulae.visualization,org.formulae.text.string,org.formulae.math.arithmetic,org.formulae.symbolic,org.formulae.list,org.formulae.relation,org.formulae.color,org.formulae.time">
	<expression tag="Formulae.Script.Row" type="0">
		<expression tag="Typesetting.MultiParagraph">
			<expression tag="Typesetting.Rule"/>
			<expression tag="Typesetting.Centering">
				<expression tag="Graphics.RasterGraphics" Value=""/>
			</expression>
			<expression tag="Typesetting.Centering">
				<expression tag="Visualization.FontSize" Size="32">
					<expression tag="String.Text" Value="Welcome to the Fōrmulæ website"/>
				</expression>
			</expression>
			<expression tag="Typesetting.Paragraph">
				<expression tag="Typesetting.BoldChunk">
					<expression tag="String.Text" Value="Introduction"/>
				</expression>
			</expression>
			<expression tag="Typesetting.Paragraph">
				<expression tag="String.Text" Value="Fōrmulæ is a"/>
				<expression tag="Internet.UniformResourceLocator" Value="https://en.wikipedia.org/wiki/Software_framework" Description="software framework"/>
				<expression tag="String.Text" Value="for visualization, edition and manipulation of complex expressions."/>
			</expression>
			<expression tag="Typesetting.Paragraph">
				<expression tag="String.Text" Value="The expressions that Fōrmulæ can handle come from many fields of knowledge such as mathematics, physics, chemistry, etc. Since the human knowledge is large, expressions with related purposes are grouped together into units called packages. Final users have the freedom of choose the packages to be used, according to their field of work/study, or their personal preferences."/>
			</expression>
			<expression tag="Typesetting.Paragraph">
				<expression tag="String.Text" Value="Fōrmulæ is an specification, it can be realized in any programming language to be used in several environments. The most popular one is as a"/>
				<expression tag="Internet.UniformResourceLocator" Value="https://en.wikipedia.org/wiki/Web_application" Description="web application,"/>
				<expression tag="String.Text" Value="just as you are currently using it, written in Vanilla"/>
				<expression tag="Internet.UniformResourceLocator" Value="https://en.wikipedia.org/wiki/JavaScript" Description="Javascript,"/>
				<expression tag="String.Text" Value="all you need in an internet connection and your browser, no additional software is needed to be installed."/>
			</expression>
			<expression tag="Typesetting.Paragraph">
				<expression tag="String.Text" Value="Because text, paragraphs, tables, colors, images, etc. can be abstracted as expressions, Fōrmulæ can also be used to format complex documents —there is a"/>
				<expression tag="Internet.UniformResourceLocator" Value="https://en.wikipedia.org/wiki/Typesetting" Description="typesetting"/>
				<expression tag="String.Text" Value="package—. The document you are reading right now looks like common HTML, but it is actually formatted in Fōrmulæ itself. The whole content of this website is made entirely of Fōrmulæ expressions."/>
			</expression>
		</expression>
	</expression>
	<expression tag="Formulae.Script.Row" type="0">
		<expression tag="Typesetting.MultiParagraph">
			<expression tag="Typesetting.Paragraph">
				<expression tag="Typesetting.BoldChunk">
					<expression tag="String.Text" Value="Visualization"/>
				</expression>
			</expression>
			<expression tag="Typesetting.Paragraph">
				<expression tag="String.Text" Value="One of the main goals of Fōrmulæ is showing expressions in two-dimensional format, just like humans do, such as"/>
				<expression tag="Math.Arithmetic.Division">
					<expression tag="Math.Arithmetic.Addition">
						<expression tag="Math.Arithmetic.Negative">
							<expression tag="Symbolic.Symbol" Name="b"/>
						</expression>
						<expression tag="Math.Arithmetic.SquareRoot">
							<expression tag="Math.Arithmetic.Addition">
								<expression tag="Math.Arithmetic.Exponentiation">
									<expression tag="Symbolic.Symbol" Name="b"/>
									<expression tag="Math.Number" Value="2"/>
								</expression>
								<expression tag="Math.Arithmetic.Negative">
									<expression tag="Math.Arithmetic.Multiplication">
										<expression tag="Math.Number" Value="4"/>
										<expression tag="Symbolic.Symbol" Name="a"/>
										<expression tag="Symbolic.Symbol" Name="c"/>
									</expression>
								</expression>
							</expression>
						</expression>
					</expression>
					<expression tag="Math.Arithmetic.Multiplication">
						<expression tag="Math.Number" Value="2"/>
						<expression tag="Symbolic.Symbol" Name="a"/>
					</expression>
				</expression>
				<expression tag="String.Text" Value=". This is also called"/>
				<expression tag="Internet.UniformResourceLocator" Value="https://en.wikipedia.org/wiki/Prettyprint" Description="pretty-print."/>
				<expression tag="String.Text" Value="Mathematical operations, matrices, trees, etc. are shown in such that way."/>
			</expression>
			<expression tag="Typesetting.Paragraph">
				<expression tag="String.Text" Value="In fact, Fōrmulæ can be used to create new useful notations. As an example, take a look at"/>
				<expression tag="Internet.UniformResourceLocator" Value="/?script=articles/Arraization" Description="arraization."/>
			</expression>
		</expression>
	</expression>
	<expression tag="Formulae.Script.Row" type="0">
		<expression tag="Typesetting.MultiParagraph">
			<expression tag="Typesetting.Paragraph">
				<expression tag="Typesetting.BoldChunk">
					<expression tag="String.Text" Value="Edition"/>
				</expression>
			</expression>
			<expression tag="Typesetting.Paragraph">
				<expression tag="String.Text" Value="Edition is the process of create an expression, or alter or delete parts (subexpressions) of an given expression."/>
			</expression>
			<expression tag="Typesetting.Paragraph">
				<expression tag="String.Text" Value="Edition is performed in small parts called"/>
				<expression tag="Typesetting.ItalicChunk">
					<expression tag="String.Text" Value="editions,"/>
				</expression>
				<expression tag="String.Text" Value="which contains the code for making a very specific change in the expression. There are also"/>
				<expression tag="Typesetting.ItalicChunk">
					<expression tag="String.Text" Value="actions,"/>
				</expression>
				<expression tag="String.Text" Value="which contains code for an specific kind of expression. For example, for the"/>
				<expression tag="Typesetting.ItalicChunk">
					<expression tag="String.Text" Value="hyperlink"/>
				</expression>
				<expression tag="String.Text" Value="expression, there is an action (triggered when you press the space key when the link is selected) that opens the link in a new tab of your web browser."/>
			</expression>
			<expression tag="Typesetting.Paragraph">
				<expression tag="String.Text" Value="For more information, please refer to the"/>
				<expression tag="Internet.UniformResourceLocator" Value="/?script=articles/Edition" Description="edition tutorial."/>
			</expression>
		</expression>
	</expression>
	<expression tag="Formulae.Script.Row" type="0">
		<expression tag="Typesetting.MultiParagraph">
			<expression tag="Typesetting.Paragraph">
				<expression tag="Typesetting.BoldChunk">
					<expression tag="String.Text" Value="Transformation (reduction)"/>
				</expression>
			</expression>
			<expression tag="Typesetting.Paragraph">
				<expression tag="String.Text" Value="Fōrmulæ framework includes a set of definitions for manipulating expressions in order to be converted into another ones. It works using"/>
				<expression tag="Typesetting.BoldChunk">
					<expression tag="String.Text" Value="reduction rules."/>
				</expression>
				<expression tag="String.Text" Value="As a very simple, but representative example, the expression"/>
				<expression tag="Visualization.Bold" Value="True" Set="True">
					<expression tag="Math.Arithmetic.Addition">
						<expression tag="Math.Number" Value="2"/>
						<expression tag="Math.Number" Value="3"/>
					</expression>
				</expression>
				<expression tag="String.Text" Value="is converted into the expression"/>
				<expression tag="Visualization.Bold" Value="True" Set="True">
					<expression tag="Math.Number" Value="5"/>
				</expression>
				<expression tag="String.Text" Value="because there is a rule —addition of numbers— that contains the code to do such that transformation."/>
			</expression>
			<expression tag="Typesetting.Paragraph">
				<expression tag="String.Text" Value="There can be multiple reduction rules for a given expression. There are expressions with no reduction rules. Numbers, strings or time expressions have no"/>
				<expression tag="Typesetting.ItalicChunk">
					<expression tag="String.Text" Value="reducers"/>
				</expression>
				<expression tag="String.Text" Value="—reduction rules—, they are not transformed into anything else. Lists have no reducers, however, its elements could, so the expression"/>
				<expression tag="Visualization.Bold" Value="True" Set="True">
					<expression tag="List.List">
						<expression tag="Math.Arithmetic.Addition">
							<expression tag="Math.Number" Value="1"/>
							<expression tag="Math.Number" Value="2"/>
						</expression>
						<expression tag="Math.Arithmetic.Addition">
							<expression tag="Math.Number" Value="3"/>
							<expression tag="Math.Number" Value="4"/>
						</expression>
					</expression>
				</expression>
				<expression tag="String.Text" Value="is converted —reduced— to the expression"/>
				<expression tag="Visualization.Bold" Value="True" Set="True">
					<expression tag="List.List">
						<expression tag="Math.Number" Value="3"/>
						<expression tag="Math.Number" Value="7"/>
					</expression>
				</expression>
				<expression tag="String.Text" Value=". Even though a list has no reducers, it can be the operand of a reducer, i.e the expression"/>
				<expression tag="Visualization.Bold" Value="True" Set="True">
					<expression tag="Relation.Max">
						<expression tag="List.List">
							<expression tag="Math.Number" Value="1"/>
							<expression tag="Math.Number" Value="5"/>
							<expression tag="Math.Number" Value="10"/>
						</expression>
					</expression>
				</expression>
				<expression tag="String.Text" Value="reduces to the expression"/>
				<expression tag="Visualization.Bold" Value="True" Set="True">
					<expression tag="Math.Number" Value="10"/>
				</expression>
				<expression tag="String.Text" Value="."/>
			</expression>
		</expression>
	</expression>
	<expression tag="Formulae.Script.Row" type="0">
		<expression tag="Typesetting.MultiParagraph">
			<expression tag="Typesetting.Paragraph">
				<expression tag="Typesetting.BoldChunk">
					<expression tag="String.Text" Value="The Fōrmulæ programming language"/>
				</expression>
			</expression>
			<expression tag="Typesetting.Paragraph">
				<expression tag="String.Text" Value="Much knowledge —specially in"/>
				<expression tag="Internet.UniformResourceLocator" Value="https://en.wikipedia.org/wiki/Formal_science" Description="formal sciences—,"/>
				<expression tag="String.Text" Value="can be abstracted as expressions. It also applies to"/>
				<expression tag="Internet.UniformResourceLocator" Value="https://en.wikipedia.org/wiki/Computer_programming" Description="computer programming."/>
				<expression tag="String.Text" Value="There is a Fōrmulæ package for"/>
				<expression tag="Internet.UniformResourceLocator" Value="https://en.wikipedia.org/wiki/Structured_programming" Description="structured programming."/>
				<expression tag="String.Text" Value="This emerging language is"/>
				<expression tag="Internet.UniformResourceLocator" Value="/?script=examples/Universal_Turing_machine" Description="proven"/>
				<expression tag="String.Text" Value="to be"/>
				<expression tag="Internet.UniformResourceLocator" Value="https://en.wikipedia.org/wiki/Turing_completeness" Description="Turing-complete,"/>
				<expression tag="String.Text" Value="in other words, it is able to run every possible algorithm."/>
			</expression>
			<expression tag="Typesetting.Paragraph">
				<expression tag="String.Text" Value="The Fōrmulæ programming language has the following features:"/>
			</expression>
			<expression tag="Typesetting.BulletedList">
				<expression tag="Typesetting.Paragraph">
					<expression tag="String.Text" Value="Because you decide which packages to load and to use, you can use the Fōrmulæ language to create and inter-operate with expressions from these packages."/>
				</expression>
				<expression tag="Typesetting.Paragraph">
					<expression tag="String.Text" Value="Since the expressions for programming —as well as the expressionf for the packages you choose— are pretty-print, it makes the Fōrmulæ language a"/>
					<expression tag="Internet.UniformResourceLocator" Value="https://en.wikipedia.org/wiki/Visual_programming_language" Description="visual programming language."/>
				</expression>
				<expression tag="Typesetting.Paragraph">
					<expression tag="String.Text" Value="You can write, edit and run programs directly in you browser, there is no need to install either an"/>
					<expression tag="Internet.UniformResourceLocator" Value="https://en.wikipedia.org/wiki/Integrated_development_environment" Description="integrated development environment"/>
					<expression tag="String.Text" Value="or any additional software. You can save your documents and programs as local files in order to be used any later, or to be shared with other persons."/>
				</expression>
			</expression>
			<expression tag="Typesetting.Paragraph">
				<expression tag="String.Text" Value="See (and run!) hundreds of examples of"/>
				<expression tag="Internet.UniformResourceLocator" Value="/?script=articles/Examples_by_category" Description="Fōrmulæ programs,"/>
				<expression tag="String.Text" Value="grouped by categories."/>
			</expression>
		</expression>
	</expression>
